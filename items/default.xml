<mxFrameItemTemplates>
	<demo>
		<css>
			<![CDATA[
				.demo{
					position: fixed;
					top: 50px;
					left: 50%;
					height: 180px;
					width: 300px;
					margin-left: -100px;
					border: 1px #FF0000 solid;
					text-align:center;
				}
			]]>
		</css>
		<data>
			<set>
				{
					name: "user",
					age: 27
				}
			</set>
		</data>
		<preload-once>
			<![CDATA[
				alert('preloading!');
			]]>
		</preload-once>
		<template>
			<![CDATA[
				<label>input your name here:</label>
				<input ng-model = "name" placeholder = "{$html}"/>
				<h3>Welcome {{name}}!</h3>
				<p><mxitem name = "frameInfo" version = "v0.1 Beta"></item></p>
				<p><mxitem name = "itemXML"></item></p>
			]]>
		</template>
		<callback>
			<![CDATA[
				console.log('done!');
			]]>
		</callback>
	</demo>

	<frameInfo>
		<template>
			<![CDATA[
				mxRender {$version}<br />with argular.js
			]]>
		</template>
	</frameInfo>

	<itemXML>
		<template>
			<![CDATA[
				<a href = "items/default.xml">Click here to see item code.</a>
			]]>
		</template>
	</itemXML>
</mxFrameItemTemplates>