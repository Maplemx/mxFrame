<mxFrameItemTemplates>
	<demo>
		<css>
			<![CDATA[
				.demo{
					position: fixed;
					top: 50px;
					left: 50%;
					height: 180px;
					width: 300px;
					margin-left: -100px;
					border: 1px #FF0000 solid;
					text-align:center;
				}
			]]>
		</css>
		<preload-once>
			<![CDATA[
				alert('preloading!');
			]]>
		</preload-once>
		<template>
			<![CDATA[
				<label>input your name here:</label>
				<input placeholder = "{$html}" id = "input_yourname"/>
				<h3>Welcome <span id = "span_yourname"></span>!</h3>
				<p><mxitem name = "frameInfo" version = "v0.1 Beta"></item></p>
				<p><mxitem name = "itemXML"></item></p>
			]]>
		</template>
		<callback>
			<![CDATA[
				$('#input_yourname').on('keyup',function(){
					$('#span_yourname').html($(this).val());
				});
				console.log('done!');
			]]>
		</callback>
	</demo>

	<frameInfo>
		<data>
			<set>
				<![CDATA[
					{version: "v0.2 Beta"}
				]]>
			</set>
		</data>
		<template>
			<![CDATA[
				mxRender {$set.version}
			]]>
		</template>
	</frameInfo>

	<itemXML>
		<template>
			<![CDATA[
				<a href = "items/default.xml">Click here to see item code.</a>
			]]>
		</template>
	</itemXML>
</mxFrameItemTemplates>